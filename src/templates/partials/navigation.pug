- var NavItems = [{'url':'home','title':'Акции','key':'home'},{'url':'category','title':'Суши','key':'category'}];



nav.main-nav
	//- a(href="#").main-nav__link Акции
	//- a(href="category.html").main-nav__link Суши
	//- a(href="#").main-nav__link Пицца
	//- a(href="#").main-nav__link Бургеры
	//- a(href="#").main-nav__link Боксы
	//- a(href="#").main-nav__link Десерты
	//- a(href="#").main-nav__link Напитки
	each page,i in NavItems
		if PageID === page.url
			a.main-nav__link(class="active", href= + page.url + '.html')
				span.name= page.title
		else
			a.main-nav__link(href= page.url + '.html')
				span.name= page.title
	a(href="#").main-nav__link Пицца
	a(href="#").main-nav__link Бургеры
	a(href="#").main-nav__link Боксы
	a(href="#").main-nav__link Десерты
	a(href="#").main-nav__link Напитки

include mobileToggle
